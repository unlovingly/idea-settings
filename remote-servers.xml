      <configuration>
        <option name="apiUrl" value="tcp://arch.wsl:2375" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="L:" />
              <option name="virtualMachinePath" value="/" />
